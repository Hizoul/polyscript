{
  "name": "isofw-web",
  "version": "1.0.1",
  "main": "index.js",
  "license": "AGPL-3.0",
  "scripts": {
    "start": "npm run dev",
    "build": "./node_modules/.bin/webpack --config ./webpack.config.prod.ts || true",
    "buildSsr": "./node_modules/.bin/webpack --config ./webpack.config.server.prod.ts  || true",
    "buildElectron": "./node_modules/.bin/webpack --config ./webpack.config.electron.ts  || true",
    "dev": "cross-env ../ci/node_modules/.bin/webpack-dev-server --config ./webpack.config.ts --port 4201 --content-base webpackDist",
    "devSsr": "./node_modules/.bin/webpack -w --config ./webpack.config.server.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@xpfw/data": "1.0.0-beta.2",
    "@xpfw/data-feathers": "1.0.0-beta.2",
    "@xpfw/form": "1.0.0-beta.2",
    "@xpfw/form-shared": "^0.1.5",
    "@xpfw/form-web": "1.0.0-beta.2",
    "@xpfw/ui-feathers": "^0.1.1",
    "@xpfw/ui-shared": "^0.1.1",
    "@xpfw/validate": "^0.1.1",
    "ajv": "^6.9.1",
    "css-loader": "^2.1.0",
    "framework7": "^3.6.5",
    "framework7-icons": "^2.1.1",
    "framework7-react": "^3.6.5",
    "localforage": "^1.7.3",
    "lodash-es": "^4.17.11",
    "mobx": "^5.9.0",
    "mobx-react-lite": "^1.0.1",
    "moment": "^2.23.0",
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "resub": "^1.0.13",
    "resub-persist": "^1.2.8",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^4.4.2",
    "@types/extract-text-webpack-plugin": "^3.0.3",
    "@types/graceful-fs": "^4.1.2",
    "@types/jest": "^23.3.12",
    "@types/lodash": "^4.14.118",
    "@types/lodash-es": "^4.17.2",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.4.22",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^4.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "graceful-fs": "^4.1.15",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "react-test-renderer": "^16.7.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "typescript": "3.1.6",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
      "isofw-node(.*)": "<rootDir>/../isofw-node$1",
      "isofw-shared(.*)": "<rootDir>/../isofw-shared$1",
      "isofw-web(.*)": "<rootDir>/../isofw-web$1",
      "isofw-rn(.*)": "<rootDir>/../isofw-rn$1",
      "\\.(css|sass|scss)$": "<rootDir>/__mocks__/styleMock.ts",
      "@xpfw/validate": "<rootDir>/node_modules/@xpfw/validate",
      "@xpfw/form-shared": "<rootDir>/node_modules/@xpfw/form-shared",
      "@xpfw/ui-shared": "<rootDir>/node_modules/@xpfw/ui-shared",
      "@xpfw/ui-tests": "<rootDir>/node_modules/@xpfw/ui-tests",
      "@xpfw/ui-feathers": "<rootDir>/node_modules/@xpfw/ui-feathers",
      "@xpfw/test-util": "<rootDir>/node_modules/@xpfw/test-util"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
