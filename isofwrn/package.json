{
  "name": "isofwrn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "dev": "./node_modules/.bin/react-native start",
    "buildAndroid": "cd android && ./gradlew assembleRelease",
    "rns": "adb reverse tcp:4202 tcp:4202",
    "rnad": "adb shell input keyevent 82"
  },
  "dependencies": {
    "react": "16.6.1",
    "react-native": "0.57.4"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "@types/react": "^16.7.2",
    "@types/react-dom": "^16.0.9",
    "@types/react-native": "^0.57.7",
    "babel-jest": "23.6.0",
    "babel-loader": "^8.0.4",
    "babel-preset-react-native": "^5",
    "file-loader": "^2.0.0",
    "haul": "^1.0.0-rc.10",
    "jest": "23.6.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.6.1",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1"
  },
  "jest": {
    "preset": "react-native",
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "\\.(css|sass|scss)$": "<rootDir>/__mocks__/styleMock.ts",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
      "isofw-node(.*)": "<rootDir>/../isofw-node$1",
      "isofw-shared(.*)": "<rootDir>/../isofw-shared$1",
      "isofw-web(.*)": "<rootDir>/../isofw-web$1",
      "isofw-rn(.*)": "<rootDir>/../isofw-rn$1"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib",
      "<rootDir>/dist"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
